{
  "name": "Sinatra",
  "version": "0.1.0",
  "description": "Sinatra startpoint",
  "main": "index.js",
  "scripts": {
    "postinstall": "./core/scripts/npm-postinstall.sh",
    "start": "npm run app-start --silent",
